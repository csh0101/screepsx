{
  "name": "screeps-typescript",
  "version": "1.0.0",
  "description": "Advanced Screeps AI written in TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "push": "rollup -c --environment DEST:main",
    "local": "rollup -c --environment DEST:local",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "screeps",
    "typescript",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.8",
    "@types/screeps": "^3.3.7",
    "@types/source-map": "^0.5.7",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "rollup": "^2.64.0",
    "rollup-plugin-screeps": "^1.0.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "source-map": "0.6.1",
    "typescript": "^4.5.4"
  }
}
